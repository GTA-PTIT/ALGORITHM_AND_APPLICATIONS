#include <bits/stdc++.h>
using namespace std;

#define quick() ios_base::sync_with_stdio(false); cin.tie(0);
#define pb push_back
using ll = long long;
int mod = 1000000007;

void Run()
{
    int test;
    cin >> test;
    while(test--)
    {
        int n;
        cin >> n;
        int sum = 0;
        while(n)
        {
            sum += n % 10;
            n /= 10;
        }
        cout << sum << endl;
    }
}

int main(){
    quick();
    Run();
}
